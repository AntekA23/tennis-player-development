  "views": {
    "public.player_basic_stats": {
      "columns": {
        "player_id": {
          "name": "player_id",
          "type": "integer",
          "primaryKey": false,
          "notNull": false
        },
        "total_matches": {
          "name": "total_matches",
          "type": "integer",
          "primaryKey": false,
          "notNull": false
        },
        "wins": {
          "name": "wins",
          "type": "integer",
          "primaryKey": false,
          "notNull": false
        },
        "win_rate": {
          "name": "win_rate",
          "type": "integer",
          "primaryKey": false,
          "notNull": false
        },
        "last_match_date": {
          "name": "last_match_date",
          "type": "timestamp with time zone",
          "primaryKey": false,
          "notNull": false
        }
      },
      "definition": "\n  SELECT \n    player_id,\n    COUNT(*) as total_matches,\n    COUNT(CASE WHEN winner_id = player_id THEN 1 END) as wins,\n    ROUND(\n      COUNT(CASE WHEN winner_id = player_id THEN 1 END)::numeric / \n      NULLIF(COUNT(*), 0) * 100\n    )::integer as win_rate,\n    MAX(start_time) as last_match_date\n  FROM (\n    SELECT player1_id as player_id, winner_id, start_time FROM match_results mr \n    JOIN calendar_events ce ON mr.event_id = ce.id\n    UNION ALL\n    SELECT player2_id as player_id, winner_id, start_time FROM match_results mr \n    JOIN calendar_events ce ON mr.event_id = ce.id\n  ) player_matches\n  GROUP BY player_id\n",
      "name": "player_basic_stats",
      "schema": "public",
      "isExisting": false,
      "materialized": false
    }
  },
